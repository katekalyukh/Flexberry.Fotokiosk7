<h3 class="ui header">{{t "forms.i-i-s-fotokiosk7-печать-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-fotokiosk7-печать-e.сделатьФото-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.сделатьФото
      relationName="сделатьФото"
      projection="СделатьФотоL"
      title=(t "forms.i-i-s-fotokiosk7-печать-e.сделатьФото-caption")
      readonly=readonly
      componentName="сделатьФотоLookup"
      class=(if (v-get validationModel "сделатьФото" "isInvalid") "error")
      data-test-i-i-s-fotokiosk7-печать-e-сделатьФото=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сделатьФото" "message")}}
  </div>
  <div class="field" data-test-i-i-s-fotokiosk7-печать-e-сделатьФото-категорФото="true">
    <label>{{t "forms.i-i-s-fotokiosk7-печать-e.сделатьФото.категорФото-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-fotokiosk7-категория")
      value=model.сделатьФото.категорФото
      class=(if (v-get validationModel.сделатьФото "категорФото" "isInvalid") "error")
      readonly=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.сделатьФото "категорФото" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.размер
      class=(if (v-get validationModel "размер" "isInvalid") "error")
      label=(t "forms.i-i-s-fotokiosk7-печать-e.размер-caption")
      data-test-i-i-s-fotokiosk7-печать-e-размер=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "размер" "message")}}
  </div>
</form>
